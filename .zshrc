. ~/.bashrc
. ~/projects/powerlevel9k/powerlevel9k.zsh-theme

SHELL=/bin/zsh

vaulted_status() {
  if [[ -z "${VAULTED_ENV}" ]]; then
    echo -n ""
  else
    exp=$(date -j -f "%Y-%m-%dT%H:%M:%S%z" "${VAULTED_ENV_EXPIRATION/Z/+0000}" +%s)
    now=$(date +%s)
    rem=$(date -j -f "%s" "$(($exp - $now))" +%H:%M:%S)
    echo -n "${VAULTED_ENV} (${rem})"
  fi
}

POWERLEVEL9K_LEFT_PROMPT_ELEMENTS=(user dir vcs custom_vaulted_status)
POWERLEVEL9K_DIR_BACKGROUND='015'
POWERLEVEL9K_PROMPT_ON_NEWLINE=true
